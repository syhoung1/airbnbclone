<div class="room-show-container">
  <%= form_for @home_listing, url: wizard_path, method: :put do |f| %>
    <%= f.label :summary %>
    <%= f.text_field :summary, placeholder: "Describe the decor, light, what's nearby, etc..." %>
    <%= f.label :good_for %>
    <%= f.select :good_for, ['Big Groups', 'Kids', 'Pets', 'Families'] %>
    <%= f.submit "Next" %>
  <% end %>
</div>
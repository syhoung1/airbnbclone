<div class="room-show-container">
  <%= form_for @home_listing, url: wizard_path, method: :put do |f| %>
    <%= f.label :capacity do %>
      <p>How many guests can your place accommodate?</p>
    <% end %>
    <%= f.number_field :capacity %>
    <%= f.label :bedrooms do %>
      <p>How many bedrooms can your guests use?</p>
    <% end %>
    <%= f.number_field :bedrooms %>
    <%= f.label :beds do %>
      <p>How many beds can your guests use?</p>
    <% end %>
    <%= f.number_field :beds %>
    <%= f.submit "Next" %>
  <% end %>
</div>
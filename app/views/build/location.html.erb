<div class="room-show-container">
  <div class="content-container text-center">
    <%= form_for @home_listing.address, url: wizard_path, method: :put do |f|%>
      <%= f.label :country do %>
        <h2>Country</h2>
      <% end %>
      <%= f.text_field :country, class: 'text-input', placeholder: "USA" %>
      <%= f.label :state do %>
        <h2>State</h2>
      <% end %>
      <%= f.text_field :state, class: 'text-input', placeholder: "CA" %>
      <%= f.label :city do %>
        <h2>City</h2>
      <% end %>
      <%= f.text_field :city, class: 'text-input', placeholder: "Los Angeles" %>
      <%= f.label :zip_code do %>
        <h2>Zip Code</h2>
      <% end %>
      <%= f.text_field :zip_code, class: 'text-input', placeholder: "91210" %>
      <%= f.label :address do %>
        <h2>Address</h2>
      <% end %>
      <%= f.text_field :address, class: 'text-input', placeholder: "123 Fake Rd" %>
      <%= f.submit "Next", class: 'submit-btn' %>
    <% end %>
  </div>
</div>

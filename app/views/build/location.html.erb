<div class="room-show-container">
  <%= form_for @home_listing.address, url: wizard_path, method: :put do |f|%>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :zip_code %>
    <%= f.text_field :zip_code %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.submit "next" %>
  <% end %>
</div>

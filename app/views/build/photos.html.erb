<div class="room-show-container">
  <div class="content-container text-center">
    <%= form_for @home_listing, url: wizard_path, method: :put do |f| %>
      <%= f.label :photos do %>
        <h2>Upload Photos here</h2>
      <% end %>
      <%= f.file_field :photos, multiple: true, class: 'file-input' %>
      <%= f.submit "Next", class: 'submit-btn' %>
      <%= link_to "Skip", next_wizard_path, class: 'submit-btn skip-btn' %>
    <% end %>
  </div>
</div>

